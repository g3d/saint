<input type="text" name="<%= element.name %>" value="<%= value || element.default %>" id="<%= element.id %>"
       style="<%= element.css_style %>" class="<%= element.css_class %> disable_enter_key"/>
<script>
    $(function() {
        $("#<%= element.id %>").datetimepicker({
            dateFormat: 'yy-mm-dd',
            timeFormat: 'hh:mm:ss.l',
            changeMonth: true,
            changeYear: true,
            showSecond: true,
            showMillisec: true
        });
    });
</script>
