<% values = (value||'').split(element.join_with)
   (options = element.options) && options.each_pair do |val, label|
       active = values.include?(val) -%>
    <label class="checkbox inline">
      <input type="checkbox" name="<%= element.name %>[]" value="<%= val %>"
             style="<%= element.css_style %>" class="<%= element.css_class %>"
             <%= 'checked' if active -%> />
      <%= label %>
    </label>
<% end -%>
